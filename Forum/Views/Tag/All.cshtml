@model IEnumerable<Tag>

<table class="table table-bordered table-hover">
    <tr>
        <th>Name</th>
        <th>Actions</th>
    </tr>
    @foreach (var tag in Model) {
        <tr>
            <td>@tag.Name</td>
            <td>
                <a class="btn btn-outline-dark" asp-action="Edit" asp-route-id="@tag.Id">Edit</a>
                <a class="btn btn-outline-dark" asp-action="Delete" asp-route-id="@tag.Id">Delete</a>
            </td>
        </tr>
    }
</table>